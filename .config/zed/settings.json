{
  "base_keymap": "VSCode",
  "agent": {
    "default_profile": "ask",
    "default_model": {
      "provider": "openrouter",
      "model": "deepseek/deepseek-chat-v3.1:free"
    }
  },
  "language_models": {
    "openai_compatible": {
      "gpt-oss": {
        "api_url": "https://openrouter.ai/api/v1",
        "available_models": [
          {
            "name": "openai/gpt-oss-20b:free",
            "display_name": null,
            "max_tokens": 200000,
            "max_output_tokens": 32000,
            "max_completion_tokens": 200000,
            "capabilities": {
              "tools": false,
              "images": false,
              "parallel_tool_calls": false,
              "prompt_cache_key": false
            }
          }
        ]
      },
      "deeseek r1": {
        "api_url": "https://openrouter.ai/api/v1",
        "available_models": [
          {
            "name": "deepseek/deepseek-r1:free",
            "display_name": null,
            "max_tokens": 200000,
            "max_output_tokens": 32000,
            "max_completion_tokens": 200000,
            "capabilities": {
              "tools": false,
              "images": false,
              "parallel_tool_calls": false,
              "prompt_cache_key": false
            }
          }
        ]
      }
    }
  },
  "show_edit_predictions": false,
  "preferred_line_length": 100,
  "features": {
    "edit_prediction_provider": "zed"
  },
  "telemetry": {
    "diagnostics": true,
    "metrics": false
  },
  "relative_line_numbers": true,
  "cursor_blink": false,
  "current_line_highlight": "gutter",
  "vim_mode": true,
  "helix_mode": false,

  "show_completions_on_input": true,
  "completions": {
    "words": "enabled",
    "lsp": true,
    "lsp_fetch_timeout_ms": 0,
    "lsp_insert_mode": "replace_suffix"
  },

  "ui_font_family": "JetBrains Mono",
  "ui_font_size": 17,
  "buffer_font_family": "JetBrains Mono",
  "buffer_font_size": 15,
  "theme": {
    "mode": "dark",
    "light": "One Light",
    "dark": "Sonokai"
  },

  "git": {
    "git_gutter": null,
    "gutter_debounce": null,
    "inline_blame": {
      "enabled": false
    },
    "hunk_style": null
  },

  "auto_signature_help": true,
  "diagnostics": {
    "button": true,
    "include_warnings": true,
    "lsp_pull_diagnostics": {
      "enabled": true,
      "debounce_ms": 50
    },
    "inline": {
      "enabled": true,
      "update_debounce_ms": 150,
      "padding": 4,
      "min_column": 0,
      "max_severity": null
    }
  },

  "scrollbar": {
    "axes": {
      "horizontal": false
    }
  },
  "tab_bar": {
    "show": false,
    "show_nav_history_buttons": false,
    "show_tab_bar_buttons": false
  }
}
